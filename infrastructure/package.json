{
  "name": "github",
  "version": "0.1.0",
  "bin": {
    "github": "bin/github.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "rm -rf cdk.out/* && cdk synth -q && find cdk.out -type f -name *.template.json -exec sed -i -z 's/\\n//g' {} + && prettier --write cdk.out"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "aws-cdk": "^2.140.0",
    "esbuild": "^0.15.15",
    "esbuild-node-externals": "^1.5.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^27.5.1",
    "prettier": "^2.8.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.8.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-sdk/client-ecs": "^3.306.0",
    "@types/cfn-response": "^1.0.5",
    "aws-cdk-lib": "^2.75.0",
    "aws-sdk": "2.1451.0",
    "cfn-response": "^1.0.1",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
